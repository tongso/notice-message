{
  "name": "tongso/notice-message",
  "description": "适用于thinkphp6的短信通知消息发送插件",
  "version" : "1.0.3",
  "minimum-stability": "dev",
  "license": "Apache-2.0",
  "homepage": "https://www.mrbyte.cn/",
  "authors": [
    {
      "name": "Lukin",
      "email": "6306643@qq.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "topthink/framework": "^6.0",
    "alibabacloud/client" : "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "topthink/think": "^6.0.0"
  },
  "autoload": {
    "psr-4": {
      "tongso\\noticeMessage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tongso\\noticeMessage\\tests\\": "tests/"
    }
  },
  "extra": {
    "think": {
      "config": {
        "noticeMessage": "src/config/config.php"
      }
    }
  }
}